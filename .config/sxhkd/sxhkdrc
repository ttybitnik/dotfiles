XF86AudioLowerVolume
	volume_down

XF86AudioRaiseVolume
	volume_up

XF86AudioMute
	pactl set-sink-mute 0 toggle; pkill -RTMIN+5 dwmblocks

XF86AudioPlay
	playerctl play-pause

XF86AudioStop
	playerctl stop

XF86AudioNext
	playerctl next

XF86AudioPrev
	playerctl previous

